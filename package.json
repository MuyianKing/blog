    "vue": "3.5.18"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "cz-git": "^1.12.0",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "shiki": "^3.9.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vitepress": "1.6.4"
