    "vue": "3.5.13"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "cz-git": "^1.11.0",
    "pagefind": "^1.3.0",
    "postcss": "^8.5.1",
    "sass": "^1.83.4",
    "shiki": "^1.27.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vitepress": "1.5.0"
